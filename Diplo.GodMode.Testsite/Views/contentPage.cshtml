@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels
@using Umbraco.Extensions
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.ContentPage>
@{
    Layout = "master.cshtml";
}

<partial name="/Views/Partials/SectionHeader.cshtml" />

<section class="section">

    <div class="container">

        <div class="col-md-3">
            <nav class="nav-bar nav-bar--list">

                <partial name="Navigation/SubNavigation"></partial>

                @await Component.InvokeAsync("RootContentList", new { maxItems = 3 })

                <vc:bananas-list></vc:bananas-list>



            </nav>
        </div>

        <div class="col-md-9">
            <article>
                @Html.GetGridHtml(Model, "bodyText", "bootstrap3-fluid")
            </article>
        </div>
    </div>

</section>